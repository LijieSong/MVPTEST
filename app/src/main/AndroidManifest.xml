<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.mvptest">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--主页-->
        <activity android:name=".main.activity.MainActivity"/>
        <!--登录页-->
        <activity android:name=".login.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--注册页-->
        <activity android:name=".register.activity.RegisterActivity"/>
        <!--视频播放页-->
        <activity android:name=".main.activity.VideoPlayActivity"
            android:theme="@style/AppTheme2"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="landscape"/>
    </application>

</manifest>